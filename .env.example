# Supabase settings (use a service_role key for writes on server side; keep this secret)
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_service_role_key

# Backend selector: 'supabase' or 'sqlite'
QIL_DB_BACKEND=supabase

# Optional: artifacts bucket (must exist in Supabase Storage)
QIL_BUCKET=artifacts
